Version = 1.4
<Listener l>
  Port = <%= @port %>
  IPv4 = true
  IPv6 = false
  SSL = true
</Listener>
LoadModule = partyline
LoadModule = webadmin
<% if @auth_type == 'sasl' -%>
LoadModule   = saslauth saslauthd
<% end -%>
